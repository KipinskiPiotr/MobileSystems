<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PKipinskiHelloWorld2.MainPage">

    <StackLayout>
        <Entry x:Name="entry"></Entry>
        <Button Text="Show Message" Clicked="ShowMessage"></Button>
        <Button Text="Run How Many Fingers" Clicked="runHowManyFingers"></Button>
        <Button Text="Run Dices" Clicked="runDices"></Button>
        <Button Text="Run BMI Calculator" Clicked="runBmiCalculator"></Button>
        <Button Text="Run Flower List" Clicked="runFlowerList"></Button>
    </StackLayout>

</ContentPage>
